// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Transaction {
    id        Int      @id @default(autoincrement())
    status    String
    hash String @unique
    type String
    block Int
    age Int
    max_fee String
    sender_address  String
    nonce String
    version String
    signature Json
    l1_gas_price String
}